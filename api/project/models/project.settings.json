{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "start_date": {
      "type": "date"
    },
    "active": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "Description": {
      "type": "richtext",
      "required": false
    },
    "department": {
      "model": "department",
      "via": "projects"
    }
  }
}
